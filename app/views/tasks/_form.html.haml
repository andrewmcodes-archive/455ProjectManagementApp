= form_for @task, url: tenant_project_tasks_url do |f|
  - if @task.errors.any?
    #error_explanation
      %h2= "#{pluralize(@task.errors.count, "error")} prohibited this task from being saved:"
      %ul
        - @task.errors.full_messages.each do |message|
          %li= message

  .field
    = f.label :title
    = f.text_field :title
  .field
    = f.label :decription
    = f.text_field :decription
  .field
    = f.label :completed
    = f.date_select :completed
  = f.hidden_field :project_id
  .actions
    = f.submit 'Save'
